<?php

$properties = $surface->Get('properties');

?>

<?php if(count($properties)) { foreach($properties as $p) { ?>
	<dl class="<?php echo $p->GetInfoTagString() ?>">
	<dt><?php echo $p->Name ?></dt>
	<dd><?php echo $p->ParseInfo() ?></dd>
	</dl>
<?php } } else { ?>
	<div class="empty">No properties.</div>
<?php } ?>
